{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "watch": [
    ".env",
    "./src/**/*"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "ts,json,env",
  "exec": "clear && npm run clean:build && node --env-file=./.env --expose-gc --trace-warnings ."
}